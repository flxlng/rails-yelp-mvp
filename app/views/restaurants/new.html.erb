<h1>Add a Restaurant</h1>
<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
  <div class=mb-3>
    <%= f.select :category, collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
  </div>


  <%= f.submit %>
<% end %>

<%= link_to 'Back to all restaurants', restaurants_path %>
